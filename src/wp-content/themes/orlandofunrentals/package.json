  "description": "wordpress customizes theme",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgobatto/orlandofunrentals.git"
  },
  "license": "MIT",
  "type": "module",
  "browserslist": [
    "extends @roots/browserslist-config"
